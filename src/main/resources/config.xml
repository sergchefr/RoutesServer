        <parameter>
          <name>toz</name>
          <limitation>int</limitation>
          <description>координата z точки прибытия</description>
        </parameter>
        <parameter>
          <name>distance</name>
          <limitation>double:(0;+inf)</limitation>
          <description>расстояние между точками отбытия и прибытия</description>
        </parameter>
      </parameters>
      <flags>
        <flag>-c</flag>
      </flags>
    </command>
